import [...]//Java classes and 5687 classes


public class spinner()
{
	private I2C sensor;
	private ByteBuffer buf = ByteBuffer.allocate(5);
	public void spinner()
	{
		_spinnerSpark = new CANSparkMax(RobotMap.CAN.SPARKMAX.TURRET_NEO, CANSparkMaxLowLevel.MotorType.kBrushless);
			//I need some futher learning on how to motor
		_spinnerEncoder = _turretSpark.getEncoder();
	}
	
	public ColorSensor(I2C.Port port)
	{
		sensor = new I2C(port, 0x39); //port, I2c address    

		sensor.write(0x00, 0b00000011); //power on, color sensor on
    }

	public void spinnerSpinThree()
	{
		spinner(); //sets up spinner motors and encoders
		int endcodPos = 0; // the current position of the encoder
		while(endcodPos != endcodEndPos)
		{
			endcodPos = _spinnerEncoder.get();
			speed = limit(speed, MAX_REVERSE_SPEED, MAX_FORWARD_SPEED); //
			_turretSpark.set(speed);
		}
	}
	
	private int Spinner_Colour(int colour)
	{
		switch(colour)
		{
			
			case(1):
			sensor.read(0x16, 1, buffer);
			return buffer.get(0);
			break;
			
			case(2):
			sensor.read(0x18, 1, buffer);
			return buffer.get(0);
			break;
			
			case(3):
			sensor.read(0x18, 1, buffer);
			return buffer.get(0);
			break;
			
		}
	}
	
	public int spinnerColour()
	{
		int colour = Spinner_Colour(colour);
		
	}
}
